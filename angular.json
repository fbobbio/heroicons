{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "angular",
  "projects": {
    "heroicons": {
      "projectType": "library",
      "root": "angular/heroicons",
      "sourceRoot": "angular/heroicons/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "angular/heroicons/tsconfig.lib.json",
            "project": "angular/heroicons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "angular/heroicons/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "angular/heroicons/src/test.ts",
            "tsConfig": "angular/heroicons/tsconfig.spec.json",
            "karmaConfig": "angular/heroicons/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "angular/heroicons/tsconfig.lib.json",
              "angular/heroicons/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "playgrond": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "angular/playgrond",
      "sourceRoot": "angular/playgrond/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/playgrond",
            "index": "angular/playgrond/src/index.html",
            "main": "angular/playgrond/src/main.ts",
            "polyfills": "angular/playgrond/src/polyfills.ts",
            "tsConfig": "angular/playgrond/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "angular/playgrond/src/favicon.ico",
              "angular/playgrond/src/assets"
            ],
            "styles": [
              "angular/playgrond/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angular/playgrond/src/environments/environment.ts",
                  "with": "angular/playgrond/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "playgrond:build:production"
            },
            "development": {
              "browserTarget": "playgrond:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "playgrond:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "angular/playgrond/src/test.ts",
            "polyfills": "angular/playgrond/src/polyfills.ts",
            "tsConfig": "angular/playgrond/tsconfig.spec.json",
            "karmaConfig": "angular/playgrond/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "angular/playgrond/src/favicon.ico",
              "angular/playgrond/src/assets"
            ],
            "styles": [
              "angular/playgrond/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }},
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": false,
      "environment": "all"
    }
  },
  "defaultProject": "heroicons"
}
