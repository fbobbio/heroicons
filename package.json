{
  "name": "heroicons",
  "version": "1.0.5",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@angular/core": "^13.2.1",
    "@angular/common": "^13.2.1",
    "@angular/compiler": "^13.2.1",
    "@angular-devkit/build-angular": "^13.2.1",
    "@angular/compiler-cli": "^13.2.1",
    "ng-packagr": "^13.2.1",
    "@svgr/cli": "^5.4.0",
    "@svgr/core": "^5.5.0",
    "@vue/compiler-dom": "^3.0.5",
    "camelcase": "^6.2.0",
    "cheerio": "^1.0.0-rc.3",
    "dedent": "^0.7.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.9",
    "@types/jasmine": "^3.6.1",
    "@types/jasminewd2": "^2.0.8",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "svg-to-vue": "^0.7.0",
    "svgo": "^1.3.2",
    "typescript": "^4.5.4",
    "vue-template-compiler": "^2.6.12",
    "rxjs": "^6.6.3",
    "zone.js": "^0.11.4"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev-ng": "npm run build-angular-icons && npm run build-angular-lib-dev && cd ./angular/heroicons/dist && npm link",
    "publish-angular": "npm run build-angular-lib-prod && cd ./angular/heroicons/dist && npm pack && npm publish",
    "----- Build Angular Lib -----": "---",
    "build-angular-lib-prod": "npm run build-angular-icons && ng build heroicons --configuration production",
    "build-angular-lib-dev": "npm run build-angular-icons && ng build heroicons",
    "----- Build Icons -----": "---",
    "build-angular-icons": "npm run __build-outline && npm run __build-solid && npm run __build-angular",
    "__build-angular": "rm -rf ./angular/heroicons/dist && rm -rf ./angular/heroicons/src/lib/icons && mkdir ./angular/heroicons/src/lib/icons && node ./scripts/build-angular.js",
    "__build-react": "rm -rf ./react && mkdir ./react && mkdir ./react/solid && mkdir ./react/outline && node ./scripts/build.js react",
    "__build-vue": "rm -rf ./vue && mkdir ./vue && mkdir ./vue/solid && mkdir ./vue/outline && node ./scripts/build.js vue",
    "__build-outline": "svgo --config=svgo.outline.yaml -f ./src/outline -o ./optimized/outline --pretty --indent=2 && rm -rf ./outline && cp -R ./optimized/outline ./outline",
    "__build-solid": "svgo --config=svgo.solid.yaml -f ./src/solid -o ./optimized/solid --pretty --indent=2 && rm -rf ./solid && cp -R ./optimized/solid ./solid"
  },
  "files": [
    "react/",
    "vue/",
    "outline/",
    "solid/",
    "angular/"
  ]
}
